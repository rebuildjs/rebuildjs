{
	"name": "rebuildjs",
	"version": "0.20.3",
	"description": "Reactive esbuild...simple hackable alternative to vite for Multi Page Apps",
	"keywords": [
		"reactive",
		"multi page apps",
		"web app",
		"web server",
		"esbuild",
		"rmemo",
		"ctx-core"
	],
	"homepage": "https://github.com/rebuildjs/rebuildjs#readme",
	"bugs": {
		"url": "https://github.com/rebuildjs/rebuildjs/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rebuildjs/rebuildjs.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"app",
		"asset",
		"browser",
		"build",
		"css",
		"ctx",
		"metafile",
		"middleware",
		"server"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./app": "./app/index.js",
		"./asset": "./asset/index.js",
		"./browser": "./browser/index.js",
		"./build": "./build/index.js",
		"./css": "./css/index.js",
		"./ctx": "./ctx/index.js",
		"./metafile": "./metafile/index.js",
		"./middleware": "./middleware/index.js",
		"./server": "./server/index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "NODE_OPTIONS=--loader=esmock tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"ctx-core": "^5.4.1",
		"elysia": "^0.7.30",
		"esbuild": "^0.19.10",
		"fdir": "^6.1.1",
		"picomatch": "^3.0.1"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"esmock": "^2.6.0",
		"tsx": "^4.7.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}
